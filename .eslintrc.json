{
	"env": {
		"browser": true,
		"es2021": true,
		"node": true
	},
	"extends": [
		"next/core-web-vitals",
		"eslint:recommended",
		//"standard-with-typescript",
		"plugin:react/recommended"
	],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": [
		"react"
	],
	"rules": {
		"react/react-in-jsx-scope": "off",
		"indent": [
			"off"
		],
		"no-tabs": 0,
		"no-mixed-spaces-and-tabs": "off"
	}
	// "extends": [
	// 	"eslint: recommended",
	// 	"plugin: react/recommended",
	// 	"next"
	// ],
	// "rules": {
	// }
}